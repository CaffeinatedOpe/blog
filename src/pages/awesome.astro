---
import Layout from "../layouts/nopreflight.astro";
import { marked } from 'marked';

const response = await fetch(
	"https://raw.githubusercontent.com/CaffeinatedOpe/Awesome/refs/heads/master/README.md"
);

const markdown = await response.text();
const content = marked.parse(markdown);
---

<title>Awesome</title>

<Layout>
	<div class="mx-16">
		<article set:html={content} class="mx-4"/>
	</div>
</Layout>
